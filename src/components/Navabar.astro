---

const linksPage: {label: string; href: string}[] = [
    {
        label: 'Home', href: '/'
    },
    {
        label: 'Work', href: '#work'
    },
    {
        label: 'Resume', href: '/resume/'
    },
]
---
<div class="sticky top-0 z-30">
    <nav >
        <div class="flex items-center justify-center">
            <ul class="py-4 px-4 bg-violet-900 rounded">{ linksPage.map(({label, href})=><a href= {href} class="py-4 px-4 text-white">{label}</a>)}</ul>
        </div>
    </nav>
</div>