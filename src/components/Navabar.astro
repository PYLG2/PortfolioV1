---

const linksPage: {label: string; href: string}[] = [
    {
        label: 'Home', href: '/'
    },
    {
        label: 'Work', href: '#work'
    },
    {
        label: 'Resume', href: '/resume/'
    },
]
---
<div class="sticky top-0 z-30">
    <nav id="navbar" class="grid grid-cols-3 place-content-center">
            <div id="logo" class="px-8 py-8 h-full">
                <span class="arrow-down py-4 px-4"></span>
            </div>
            <div id="links-ancre">
            <ul class="py-2 px-2 bg-violet-900 rounded flex justify-center">{ linksPage.map(({label, href})=><a href= {href} class="py-4 px-4 text-white">{label}</a>)}</ul>
            </div>    
    </nav>
</div>

<style>


.arrow-down {
  width: 30px;
  height: 30px;
  border: 2px solid #f0eded;
  border-left: 0;
  border-top: 0;
  transform: rotate(45deg);
}
</style>